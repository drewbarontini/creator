{
  "name": "creator-multi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "autoprefixer": "postcss -u --no-map autoprefixer --autoprefixer.browsers '&gt; 5%, ie 10' -r public/stylesheets/application.css",
    "babel": "babel --presets es2015 app/assets/javascripts -o public/javascripts/application.js",
    "build:all": "npm run build:css && npm run build:js",
    "build:css": "npm run scss && npm run clean-css && npm run autoprefixer",
    "build:js": "npm run babel && npm run uglify",
    "clean-css": "cleancss -o public/stylesheets/application.css public/stylesheets/application.css",
    "icons": "svgo -f public/images/icons && svg-sprite-generate -d public/images/icons -o public/images/icons/icons.svg",
    "scss": "node-sass -x app/assets/stylesheets/application.scss public/stylesheets/application.css",
    "start": "parallelshell 'npm run watch:css' 'npm run watch:js' 'node ./bin/www'",
    "uglify": "uglifyjs public/javascripts/application.js -o public/javascripts/application.js",
    "watch:css": "onchange 'app/assets/stylesheets/*.scss' -- npm run build:css",
    "watch:js": "onchange 'app/assets/javascripts/application.js' -- npm run build:js"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "body-parser": "~1.16.0",
    "clean-css": "^4.0.9",
    "clean-css-cli": "^4.0.9",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "dotenv": "^4.0.0",
    "express": "~4.14.1",
    "express-session": "^1.15.2",
    "flash": "^1.1.0",
    "morgan": "~1.7.0",
    "node-sass": "^4.5.0",
    "onchange": "^3.2.1",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^3.0.0",
    "pug": "~2.0.0-beta10",
    "serve-favicon": "~2.3.2",
    "svg-sprite-generator": "0.0.7",
    "svgo": "^0.7.2",
    "uglify-js": "^2.8.14",
    "winston": "^2.3.1"
  },
  "devDependencies": {}
}
