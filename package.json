{
  "name": "creator-multi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "autoprefixer": "postcss -u autoprefixer --autoprefixer.browsers '&gt; 5%, ie 10' -r app/public/stylesheets/app.css",
    "babel": "babel --presets es2015 app/assets/javascripts -o app/public/javascripts/app.js",
    "build:all": "npm run build:css && npm run build:js",
    "build:css": "npm run scss && npm run clean-css && npm run autoprefixer",
    "build:js": "npm run babel && npm run uglify",
    "clean-css": "cleancss -o app/public/stylesheets/app.css app/public/stylesheets/app.css",
    "scss": "node-sass app/assets/stylesheets/app.scss app/public/stylesheets/app.css",
    "serve": "browser-sync start --proxy 'localhost:4000' --files 'app/views/*.pug, app/assets/stylesheets/*.scss, app/assets/javascripts/*.js'",
    "start": "parallelshell 'npm run serve' 'npm run watch:css' 'npm run watch:js' 'node ./bin/www'",
    "uglify": "uglifyjs app/assets/javascripts/app.js -m -o app/public/javascripts/app.js",
    "watch:css": "onchange 'app/assets/stylesheets/*.scss' -- npm run build:css",
    "watch:js": "onchange 'app/assets/javascripts/app.js' -- npm run build:js"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "body-parser": "~1.16.0",
    "browser-sync": "^2.18.8",
    "clean-css": "^4.0.9",
    "clean-css-cli": "^4.0.9",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "morgan": "~1.7.0",
    "onchange": "^3.2.1",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^3.0.0",
    "pug": "~2.0.0-beta10",
    "serve-favicon": "~2.3.2",
    "uglify-js": "^2.8.14"
  },
  "devDependencies": {
    "node-sass": "^4.5.0"
  }
}
